{
  "manifest_version": 2,
  "name": "Night Owl",
  "description": "To help reduce strain on eyes at night",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  
  "background": {
        "scripts": ["background.js"]
    },
    
    "permissions": [
        "tabs", "tabs", "http://*/*", "https://*/*"
    ]
}